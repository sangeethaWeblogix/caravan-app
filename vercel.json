{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/wp-admin(/?)(.*)",
      "dest": "https://api.dev.caravansforsale.com.au/wp-admin/$2",
      "headers": {
        "Host": "api.dev.caravansforsale.com.au" 
      }
    },
    {
      "src": "/wp-json(/?)(.*)",
      "dest": "https://api.dev.caravansforsale.com.au/wp-json/$2",
      "headers": {
        "Host": "api.dev.caravansforsale.com.au"
      }
    },
    {
      "src": "/wp-includes(/?)(.*)",
      "dest": "https://www.api.dev.caravansforsale.com.au/wp-includes/$2",
       "headers": {
        "Host": "api.dev.caravansforsale.com.au"
      }
    },
    {
      "src": "/wp-content(/?)(.*)",
      "dest": "https://www.api.dev.caravansforsale.com.au/wp-content/$2",
       "headers": {
        "Host": "api.dev.caravansforsale.com.au"
      }
    }
  ]
}
